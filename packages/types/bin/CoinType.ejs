export enum CoinType {
    <% coins.forEach((coin) => { -%>
        <%-coin.id%> = <%-coin.code%>,
    <% }) %>
}

<% methods.forEach((method) => { -%>
    export function <%-method.name%> (coin: CoinType): <%-method.returnType%> {
        switch (coin) {
            <% coins.forEach((coin) => { -%>
                case CoinType.<%-coin.id%>: <%-method.body(coin)%>;
            <% }) %>
        }
    }
<% }) %>